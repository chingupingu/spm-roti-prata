{
  "version": 2,
  "builds": [
    {
      "src": "RotiPortal/backend/run.py",
      "use": "@vercel/python"
    },
    {
      "src": "RotiPortal/frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "RotiPortal/backend/run.py"
    },
    {
      "src": "/home",
      "dest": "RotiPortal/frontend/dist/index.html"
    },
    {
      "src": "/",
      "dest": "RotiPortal/frontend/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "RotiPortal/frontend/dist/index.html"
    }
  ]
}