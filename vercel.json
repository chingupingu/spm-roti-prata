{
  "version": 2,
    "builds": [
    {
      "src": "RotiPortal/backend/run.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "maxDuration": "15mb"
      }
    },
    {
      "src": "RotiPortal/frontend/dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "RotiPortal/backend/run.py"
    },
    {
      "src": "/(.*)",
      "dest": "RotiPortal/frontend/dist/index.html"
    }
  ]
}